@using LiXinLanguage
@{
    ViewBag.Title = CommonLanguage.Common_NoRight;
    Layout = null;
    string backUrl = ViewBag.backUrl;
    if (backUrl!=null && !backUrl.StartsWith("/"))
    {
        backUrl = "/" + backUrl;
    }
}
<!DOCTYPE HTML>
<html id="bg">
    <head>
        <title>@Session["errorString"]</title>
        <meta http-equiv="Refresh" content="1;url=/Login/Index" />@*?backUrl=@Server.UrlEncode(backUrl)*@
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <link href="@Url.Content("~/Content/Raster.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/base.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/index.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/type.css")" rel="stylesheet" type="text/css" />
        <script src="@Url.Content("~/Scripts/jquery-1.8.1.min.js")" type="text/javascript"> </script>
    </head>
    <body>
    <div id="record">
    <div class="error">
        <h2><span class="red">提示：</span>@(Session["errorString"] == null ? "出错了" : (Session["errorString"].ToString() == "" ? "出错了" : Session["errorString"]))</h2>
    </div>
    </div>
    </body>
</html>