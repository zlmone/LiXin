
@{
    Layout = null;
}

    <div class="main-c">
        <table class="tab-List">
            <thead>
                <tr>
                    <th class="span4">序号</th>
                    <th class="span12">姓名</th>
                    <th>部门/分所</th>
                    <th class="span10">考勤状态</th>
                </tr>
            </thead>
            <tbody id="userlist">
            </tbody>
            <tfoot>
            </tfoot>
        </table>
        
    </div>
    <script type="text/javascript">
        $(document).ready(function () {
            fOnload();
        });
        function fOnload() {
            $("#userlist").JsRenderData({
                sourceUrl: '/DepCourseOpenOthersCheck/FUserrAttendceList?courseId=@ViewBag.courseid' ,
                isPage: true,
                pageSize: 20,
                pageIndex: 1,
                templateID: "CourseListTemplate"
            }); 
        }
    </script>
    <script id="CourseListTemplate" type="text/x-jsrender">
        {{for #data}}
         <tr>
            <td>{{:#index+1}}</td>               
            <td>{{>Realname}}</td>
            <td><div class="ovh span36 tl" title="{{>DeptName}}">{{>DeptName}}</div></td>
            <td>{{:StatusStr}}</td> 
        </tr> 
        {{/for}}
</script>
