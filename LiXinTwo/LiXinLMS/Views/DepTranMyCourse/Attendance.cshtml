@{
    ViewBag.Title = "Attendance";
    Layout = null;
    var Cl_Attendce = ViewBag.Cl_Attendce as LiXinModels.DepTranAttendce.DepTran_Attendce;
    var cocurse = ViewBag.cocurse as LiXinModels.CourseManage.Co_Course;
    ViewBag.Title = "课程详情";
}

<div class="main-c">
            @*@(Html.Action("SiteMapLink", "Common", new { linkName = "课程详情" }))
            @Html.Action("MyCourse", "MyCourse", new { id = @ViewBag.Id })*@
            
            <table class="tab-Form">
                

                 @if (ViewBag.Cl_Attendce == null && ViewBag.backFrom==0)
                { 
                <tr><td>
                        <div class="alert alert-info" >                        
