@{
    ViewBag.Title = "胜任力课程体系";
    if (ViewBag.ShowLayout==1)
    {
        Layout = null;
    }
    var sid = Request.QueryString["sid"] ?? "0";
}
<div class="main-c">
     @(Html.Action("SiteMapLink", "Common", new
 {
     linkName = "胜任力课程体系"
 }))
    <div id="div_courseSystem" class="all20 fl"></div>
    <div id="div_CompetencyCourseList" class="all79 fr"></div>
</div>
<script type="text/javascript">    
    $(document).ready(function () {
        $.get("/CourseManage/CompetencyCourseList?showGrade=1", function (data) { $("#div_CompetencyCourseList").html(data); });
        $.get("/CourseManage/CourseSystemTree?checkBoxFlag=1&sid=@sid", function (data) { $("#div_courseSystem").html(data); });
    });
</script>
