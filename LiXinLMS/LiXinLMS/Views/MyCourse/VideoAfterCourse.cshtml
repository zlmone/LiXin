@using LiXinModels.CourseManage
@using LiXinModels.Survey
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    //var qaaList = ViewBag.qaaList as List<Survey_QuestionAndAnswer>;
   // var id = Request.QueryString["id"].ToString();
    ViewBag.Title = "课程详情";
    var flag = Request.QueryString["flag"] ?? "0";//为1时来自首页
}

<!DOCTYPE html>

<html>
<head>
    <title></title>
</head>
    <body>
        <div class="main-c">
            @(Html.Action("SiteMapLink", "Common", new
        {
            linkName = "课程详情"
        }))
            <input type="hidden" id="txt_CourseId" value="@ViewBag.Id"/><!--存放课程id-->
            @Html.Action("VideoCourse", "MyCourse", new { courseid = ViewBag.id, flag = flag, backurl = ViewBag.backurl+"?p=1" })       
            <div>
                @Html.Action("AfterCourseNewList", "MyCourse", new { id = ViewBag.id, backurl = ViewBag.backurl+"?p=1"})
            </div>
        
        </div>
    </body>
    <script type="text/javascript">
        $(document).ready(function() {
            $("#ul_menu li:eq(1)").addClass("On").siblings("li").removeClass("On");

        })
    </script>    
</html>
